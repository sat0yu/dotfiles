alias -g L='| less'
alias -g G='| grep'
alias -g H='| head'
alias -g T='| tail'

alias emacs="emacs -nw"
alias ls="ls -al"
alias startvm="VBoxManage startvm ubuntu --type=headless"
alias pausevm="VBoxManage controlvm ubuntu pause"
alias resumevm="VBoxManage controlvm ubuntu resume"
alias vmlist="VBoxManage list runningvms"
